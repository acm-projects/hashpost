{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/posts",
        "function": "posts"
      },
      
      {
        "source": "/auth",
        "function": "auth"
      }
    ]
  }
}